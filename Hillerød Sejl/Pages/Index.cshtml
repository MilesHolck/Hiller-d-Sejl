@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to Hillerød sejlklub, VAMONOS</h1>
</div>



<div class="container">
    <div id="carouselExampleIndicators"
         class="carousel slide mx-auto"
         style="max-width: 1000px;"
         data-bs-ride="carousel"



<div id="carouselExample" class="carousel slide">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="~/Images/CafeBillede.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="~/Images/MastFraAgter.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="~/Images/MastView.jpg" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
    


<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js" integrity="sha384-FKyoEForCGlyvwx9Hj09JcYn3nv7wiPVlz7YYwJrWVcXK/BmnVDxM+D2scQbITxI" crossorigin="anonymous"></script>
